		"@lezer/common": "^1.0.0",
		"@lezer/highlight": "^1.0.0",
		"bauble-runtime": "^0.0.3",
		"big.js": "^6.2.1",
		"codemirror": "^6.0.1",
		"codemirror-lang-janet": "^0.2.0",
		"gl-matrix": "^3.4.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@types/big.js": "^6.1.6",
		"@types/marked": "^4.0.7",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
